# MagDB Miss List
# Generated from rhinventory

magazines:
{% for magazine_id, data in magazines.items() %}
  - id: {{ magazine_id }}
    slug: {{ data.magazine.slug | tojson }}
    title: {{ data.magazine.title | tojson }}
    {% if data.logos %}
    logos:
      {% for image in data.logos %}
      - url: {{ image.url | tojson }}
        background_color: {{ image.background_color.value or None | tojson }}
      {% endfor %}
    {% endif %}
    missing_issues:
      {% for issue in missing_magazines[magazine_id] %}
      - magazine_issue: {{ issue.magazine_issue | string | tojson }}
        name_suffix: {{ issue.name_suffix | default('') | tojson }}
        status: {{ issue.status.name | tojson }}
      {% endfor %}
{% else %}
  []
{% endfor %}
