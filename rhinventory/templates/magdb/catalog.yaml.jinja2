# MagDB Catalog
# Generated from rhinventory

magazines:
{% for magazine in magazines %}
  - id: {{ magazine.id }}
    slug: {{ magazine.slug | tojson }}
    title: {{ magazine.title | tojson }}
    description: {{ magazine.description | default('') | tojson }}
    {% if logos[magazine.id] %}
    logos:
      {% for logo in logos[magazine.id] %}
      - url: {{ logo.url | tojson }}
        background_color: {{ logo.background_color.value or None | tojson }}
      {% endfor %}
    {% endif %}
{% endfor %}
